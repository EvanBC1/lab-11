<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../layout/head') %>
</head>
<body>
  <h1>Search Results</h1>

  <ul>
   
    <%  searchResults.forEach(book => { %>
    <div class="books">
      <img src="<%= book.image_url %>" alt="">
      <li class="title"><%= book.title %></li>
      <li><%= book.authors %></li>
      <li class="description"><%= book.description %></li>
    </div>
    <% })%>
  </ul>
</body>
</html>

